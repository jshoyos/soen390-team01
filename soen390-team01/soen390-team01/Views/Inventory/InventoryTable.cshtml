@using Newtonsoft.Json
@model List<soen390_team01.Data.Entities.Inventory>

<table class="table" aria-label="Inventory">
    <tr>
        <th scope="col"><u>ID</u></th>
        <th scope="col"><u>ItemID</u></th>
        <th scope="col"><u>Type</u></th>
        <th scope="col"><u>Warehouse</u></th>
        <th scope="col"><u>Quantity</u></th>
        <th scope="col"><u>Added</u></th>
        <th scope="col"><u>Updated</u></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="@("itemRow" + item.InventoryId)">
            @{await Html.RenderPartialAsync("InventoryItem", item);}
        </tr>
    }
</table>