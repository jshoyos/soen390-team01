@using Newtonsoft.Json
@model List<soen390_team01.Data.Entities.Material>
<table class="table" width="100%">
    <tr>
        <td><u>ItemId</u></td>
        <td><u>Name</u></td>
        <td><u>Price</u></td>
        <td><u>Grade</u></td>
    </tr>
    @foreach (var item in Model)
    {
        <tr class="@("itemRow" + item.ItemId)">
            <td>@item.ItemId</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
            <td>@item.Grade</td>
        </tr>
    }
</table>